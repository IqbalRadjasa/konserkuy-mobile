<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <!-- Warna latar belakang (opsional, gunakan transparan jika hanya ingin border) -->
    <solid android:color="@color/white" /> <!-- Ganti sesuai warna -->

    <!-- Border -->
    <stroke
        android:width="14dp"
        android:color="@color/white"/> <!-- Warna border -->

    <!-- Sudut membulat (opsional) -->
    <corners android:radius="8dp" />
</shape>
